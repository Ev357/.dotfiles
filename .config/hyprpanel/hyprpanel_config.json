{
  "bar.customModules.updates.pollingInterval": 1440000,
  "theme.font.name": "GeistMono Nerd Font",
  "theme.font.size": "1.2rem",
  "theme.bar.floating": true,
  "bar.autoHide": "never",
  "theme.bar.buttons.enableBorders": false,
  "theme.bar.menus.monochrome": true,
  "theme.bar.menus.opacity": 100,
  "theme.bar.transparent": true,
  "theme.bar.margin_sides": "0.5em",
  "theme.bar.margin_top": "0.5em",
  "tear": true,
  "scalingPriority": "hyprland",
  "theme.bar.dropdownGap": "2.9em",
  "theme.bar.outer_spacing": "-0.25em",
  "theme.bar.border_radius": "0.4em",
  "theme.bar.buttons.y_margins": "0em",
  "theme.bar.buttons.spacing": "0.25em",
  "dbus.notifications.enabled": false,
  "bar.layouts": {
    "0": {
      "left": [
        "workspaces"
      ],
      "middle": [
        "media"
      ],
      "right": [
        "volume",
        "network",
        "bluetooth",
        "battery",
        "systray",
        "clock",
        "dashboard"
      ]
    }
  },
  "menus.dashboard.shortcuts.enabled": false,
  "menus.dashboard.controls.enabled": true,
  "theme.osd.enable": true,
  "menus.clock.time.military": true,
  "menus.clock.time.hideSeconds": false,
  "menus.dashboard.directories.enabled": false,
  "theme.bar.buttons.clock.enableBorder": false,
  "bar.clock.format": "%H:%M",
  "theme.bar.buttons.modules.hyprsunset.enableBorder": false,
  "wallpaper.enable": true,
  "theme.matugen": false,
  "theme.bar.buttons.monochrome": true,
  "menus.dashboard.powermenu.avatar.image": "~/.face.icon",
  "bar.workspaces.show_numbered": false,
  "bar.systray.ignore": [
    "Nextcloud"
  ]
}